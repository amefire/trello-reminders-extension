{
    "name": "Trello Reminders Extensions",
    "description": "Chrome extension for Trello Reminders",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://trello.com/*"
            ],
            "js": [
                "/output/chrome.js"
            ],
            "run_at": "document_end"
        }
    ]
}